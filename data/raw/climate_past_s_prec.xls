/* 지역관리자 aws */
#now_aws {
	position:relative; 
	background:url(/web/imgs/now_aws_bg.gif) left top no-repeat; 
	height:1028px;
	width:1280px;
}
#now_aws h1 {
	color:#fff;
	background-color:#000;
	padding-top:10px;
	font-size:43px;
	height:60px;
	text-align:center;
}
#now_aws h1 em {
	padding-left:50px;
	color:#000;
	font-size:9px;
}

#now_aws ul.info {
	margin-top:10px;
	border:1px #3876A4 solid;
	width:200px;
	background-color:#fff;
}
#now_aws ul.info li.data {
	color:#3876A4;
	padding:7px 10px;
	font-weight:bold;
	font-size:37px;
	text-align:right;
}
#now_aws ul.info li.data em {
	color:#000;
	font-size:37px;
	margin-left:10px;
}
#now_aws h2 {
	font-size:xx-large;
	/*background:url(/web/imgs/m_icon2.gif) left 3px no-repeat;*/
	background:none;
	padding-left:20px;
	height:30px;
	margin-bottom:20px;
	color:#fff;
}
#now_aws div.wd 	{	position:absolute; 	top:110px; 	left:210px;	text-align:center;	width:260px;	height:260px;	}
#now_aws div.ws 	{	position:absolute; 	top:110px; 	left:490px;	text-align:center;	width:260px;	height:260px;	}
#now_aws div.hum 	{	position:absolute; 	top:110px; 	left:770px;	text-align:center;	width:260px;	height:260px;	}
#now_aws div.temp 	{	position:absolute; 	top:110px; 	left:1020px;text-align:center;	width:260px;	height:260px;	}
#now_aws div.wd p, 
#now_aws div.ws p, 
#now_aws div.hum p, 
#now_aws div.temp p 		{	height:270px;				}
#now_aws ul.vis,
#now_aws ul.prec,
#now_aws ul.pres,
#now_aws ul.weat 			{	position:absolute; 	border:1px #3876A4 solid;	width:410px;	background-color:#fff;		}
#now_aws ul.vis 			{	top:570px; 	left:330px;		}
#now_aws ul.prec 			{	top:570px; 	left:755px;		}
#now_aws ul.pres 			{	top:700px; 	left:330px;		}
#now_aws ul.weat 			{	top:700px; 	left:755px;		}
#now_aws ul.vis  li,
#now_aws ul.prec li,
#now_aws ul.pres li,
#now_aws ul.weat li 		{	font-weight:bold;	font-size:37px;													}
#now_aws ul.vis li.factor,
#now_aws ul.prec li.factor,
#now_aws ul.pres li.factor,
#now_aws ul.weat li.factor 	{	clear:both;	background-color:#3876A4;	color:#fff;	padding:7px 20px;				}
#now_aws ul.vis li.data,
#now_aws ul.prec li.data,
#now_aws ul.pres li.data,
#now_aws ul.weat li.data 	{	float:left;	color:#3876A4;	width:300px;	text-align:right;	padding:7px 10px;	}
#now_aws ul.vis li.unit,
#now_aws ul.prec li.unit,
#now_aws ul.pres li.unit 	{	float:left;	width:50px;	text-align:center;	padding:7px 0;							}

