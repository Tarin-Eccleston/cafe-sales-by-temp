
<HTML>
<HEAD>
	<META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=utf-8" />
	
		<STYLE TYPE="text/css">/*----------------------- AWS.css ----------------------- */
body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 9pt;
	background-color: #fff;
	color: #707070;
	margin: 0;
	padding: 0;
}
/*---------------------------------------------------------*/

a 								{	color: #000;	text-decoration: none;			}
img 							{	border: none;	vertical-align: middle;			}
form, p 						{	margin:0;	padding:0;							}
em 								{	font-style: normal;								}
p.text 							{	margin: 0 0 0.75em 0;	line-height: 1.3em;		}
p.text em 						{	color:#3876A4; 	 								}
select, input, textarea 		{	color: #555;	font-size: 9pt;	vertical-align: middle;	margin-right:1px;	}
h1, h2, h3, h4 					{	color: #707070;	font-weight: normal;	margin: 0 0 0.25em 0;				}
h1 a, h2 a, h3 a, h4 a 			{	color: #707070;		}
h1 								{	font-size: 125%;	font-weight: bold;	padding-left:10px;	padding-bottom:7px;	background:url(/web/imgs/title_icon.gif) left top no-repeat;	}
h1 em							{	color:#3876A4;		}
h2 								{	font-size: 110%;	font-weight: bold;	padding-left:10px;	height:18px;		background:url(/web/imgs/m_icon.gif) left 3px no-repeat;		}
h2 em 							{	color:#3876A4;		}
h3 								{	font-size: 100%;	border-bottom: none;padding-left:15px;	margin-bottom:5px;	background:url(/web/imgs/icon_04.gif) left 50% no-repeat;		}
h3 em 							{	color:#000;		font-weight:bold;	margin-right:10px;		}
h3 span 						{	color:#3876A4;	font-weight:bold;	margin:0 5px;			}
h4 								{	font-size: 85%;	font-weight:bold;							}
h4 em 							{	color:#3876A4;												}
.button 						{	cursor: pointer;	vertical-align: middle;					}
.button2 						{	text-align:center;	vertical-align: middle;		/*	cursor: pointer;	*/		}
body.basic 						{	text-align: center;	background-color: #F6F6F6;	background:url(/web/imgs/m_bg.gif) left top repeat-x;	}
ul, li 							{	margin:0;	padding:0;	list-style: none;	}

/*----------------------- table ------------------------*/

table.type_a, 
table.type_a1 					{	border-top:2px #5A9FC8 solid;	border-bottom:1px #5A9FC8 solid;	margin-bottom: 20px;	border-collapse: collapse;	/*	width: 98%; */	font-size:90%;	}
table.type_a  thead th,
table.type_a1 thead th 			{	text-align: center;	background-color: #D5E8FC;	color: #333333;		border: #CFCFCF 1px solid;	padding-top: 3px;	padding-bottom: 3px;	}
table.type_a  thead td,
table.type_a1 thead td 			{	padding-top: 2px;	padding-bottom: 2px;	color: #707070;	background-color: #f1f1f1;	border: #CFCFCF 1px solid;	text-align:center;		}
table.type_a  thead td.cate,
table.type_a1 thead td.cate 	{	color: #306E95;	background-color: #DDF4FC;	line-height:110%;								}
table.type_a  tbody th,
table.type_a1 tbody th 			{	text-align: left;	color: #306E95;	background-color: #EFFBFF;	border: #CFCFCF 1px solid;	}
table.type_a  tbody th.cate,
table.type_a1 tbody th.cate 	{	background-color: #f5f5f5;	height:18px;	color:#707070;		}
table.type_a  tbody td 			{	color: #707070;	/*	background-color: #fff;*/	border: #CFCFCF 1px solid;	text-align:right;	height:18px;	padding:0px 1px 0px 0px;	/*	padding-right: 10px;*/	}
table.type_a1 tbody td 			{	color: #707070;	/*	background-color: #fff;*/	border: #CFCFCF 1px solid;	text-align:right;	padding-right:2px;		}
table.type_a  tbody td.l, 
table.type_a1 tbody td.l 		{	color: #707070;	/*	background-color: #fff; */	border: #CFCFCF 1px solid;	text-align:left;	padding-left:15px;		}
table.type_a  tbody td.select,
table.type_a1 tbody td.select 	{	background-color: #f5f5f5;		}
table.type_a  a:hover,
table.type_a1 a:hover 			{	color:#000;		text-decoration: underline;		}

/*--------------------------------------------------------*/

table.type_b 					{	border-top:2px #5A9FC8 solid;	border-bottom:1px #5A9FC8 solid;	margin-bottom: 20px;	border-collapse: collapse;	/*	width: 98%;*/	font-size:80%;				}
table.type_b thead th 			{	text-align: center;	color: #333;	background-color: #D5E8FC;	font-size:110%;	border: #CFCFCF 1px solid;	padding-top: 3px;	padding-bottom: 3px;	width: 102px;	}
table.type_b tbody th 			{	text-align: right;	color: #707070;	background-color: #f1f1f1;	border: #CFCFCF 1px solid;	padding-top: 4px;	padding-bottom: 4px;	padding-right:10px;				}
table.type_b tbody td 			{	padding-top: 7px;	padding-bottom: 5px;	padding-left:3px;	color: #000;	background-color: #fff;	border: #CFCFCF 1px solid;	height:70px;}
table.type_b tbody td em 		{	color:#3876A4;								}
table.type_b tbody td a 		{	color:#1663A8;								}
table.type_b tbody td a:hover 	{	color:#000;		text-decoration: underline;	}
table.type_b tbody th.red 		{	color:#BA0505;								}
table.type_b tbody th.blue		{	color:#0C55A6;								}

/*-----------------------  iframe ------------------------*/

table.type_c 				{	border-top:2px #5A9FC8 solid;	border-collapse: collapse;	font-size:90%;	line-height:100%;	/*	width: 98%; */	}
table.type_d 				{	border-collapse: collapse;	font-size:90%;	/*	width: 98%; */													}
table.type_e 				{	border-bottom:1px #5A9FC8 solid;	border-collapse: collapse;	font-size:90%;	/*	width: 98%; */				}
table.type_c thead th,
table.type_d thead th,
table.type_e thead th 		{	color: #333;	background-color: #D5E8FC;		text-align: center;	border: #CFCFCF 1px solid;	padding-top: 2px;	padding-bottom: 2px;	}
table.type_d thead th 		{	color: #333;	background-color: #D5E8FC;		text-align: center;	border: #CFCFCF 1px solid;					}

table.type_c thead td,
table.type_d thead td,
table.type_e thead td 		{	color: #707070;	background-color: #f1f1f1;		text-align:center;	border: #CFCFCF 1px solid;	height:16px;	}
table.type_c thead td.cate,
table.type_d thead td.cate,
table.type_e thead td.cate 	{	color: #306E95;	background-color: #DDF4FC;		line-height:110%;												}
table.type_c thead td.cate2,
table.type_d thead td.cate2,
table.type_e thead td.cate2 {	color: #306E95;	background-color: #DDF4FC;		text-align: right;	line-height:110%;	font-weight:normal;		}

table.type_c tbody th,
table.type_d tbody th,
table.type_e tbody th 		{	color: #306E95;	background-color: #EFFBFF;		text-align: center;	height:16px;	border: #CFCFCF 1px solid;	}
table.type_c tbody th.cate,
table.type_d tbody th.cate,
table.type_e tbody th.cate 	{	color:#707070;	background-color: #f5f5f5;																		}
table.type_c tbody th.cate2,
table.type_d tbody th.cate2,
table.type_e tbody th.cate2 {	color:#707070;	background-color: #f5f5f5;		text-align: right;	font-weight:normal;							}
table.type_c tbody th.cate3,
table.type_d tbody th.cate3,
table.type_e tbody th.cate3 {	color:#707070;	background-color: #EFFBFF;		text-align: right;	font-weight:normal;							}

table.type_c tbody td 		{	color: #707070;	/*	background-color: #fff;*/	border: #CFCFCF 1px solid;	text-align:center;					}
table.type_d tbody td 		{	color: #707070;	/*	background-color: #fff;*/	border: #CFCFCF 1px solid;	text-align:right;	height:16px;	}
table.type_e tbody td 		{	color: #707070;	/*	background-color: #fff;*/	border: #CFCFCF 1px solid;	text-align:right;	height:16px;	}

p.paging 					{	text-align:center;	font-size:90%;		}
p.paging img 				{	margin:0 5px;							}
p.paging span 				{	color: #3876A4;	font-weight:bold;		}
p.paging .unselect 			{	color: #707070;	font-weight:normal;		}

table.calendar 				{	table-layout:fixed;	line-height:100%;	}
table.calendar td.factor 	{	border:0;	height:0;	padding:0px 0;	width:32px;	color: #3876A4;						}
table.calendar td.data 		{	border:0;	height:0;	padding:0px 0;	width:45px;	color: #222;	text-align:right;	}
table.calendar td.unit 		{	border:0;	height:0;	padding:0px 0;	width:18px;	color: #222;	text-align:right;	}

/*-----------------------  popup ------------------------*/

#pop 						{	border-top:7px #3876A4 solid;	width:470px;	padding:15px;}
#pop h1 					{	height:18px;	border-bottom:1px #E9E9E9 solid;	padding-bottom:10px;	margin-bottom:20px;}
#pop h2 					{	font-size: 100%;	font-weight: bold;	padding-left:10px;	height:18px;	padding-bottom:10px;	background:none;}
#pop div.info 				{	padding-left:10px;	margin-bottom:15px;	border-bottom:1px #E9E9E9 solid;}
#pop div.info p.img 		{	text-align:center;	padding-bottom:20px;}
#pop div.info ul 			{	padding-bottom:10px;}
#pop div.info li 			{	padding-bottom:7px;	}
#pop div.info li em 		{	font-weight:bold;	color:#3876A4;	}
#pop ul.under_info 			{	background-color:#f5f5f5;	padding:10px 20px;	margin-bottom:15px;	}
#pop div.action 			{	width:100%;	text-align:center;	height:25px;	background-color:#3876A4;	}
div.info p.data_img 		{	margin-bottom:10px;	}

/*-----------------------  main ------------------------*/

#all_wrapper 						{	background:url(/web/imgs/top_bg.gif) left top repeat-x;	text-align: left;	width: 100%;	min-width: 950px;	}
#wrapper 							{	text-align: left;	background-color: #fff;	width: 100%;	padding:0;	}
#header 							{	text-align: left;	background-color: #fff;	width: 100%;	background:url(/web/imgs/top_bg.gif) left top repeat-x;	height:100px;	}
#header div.top 					{	height:50px;								}
#header div.top p.logo 				{	position:absolute;	left:17px;	top: 4px;	}
#header div.top p.top_info 			{	font-size:7pt;	text-align:right;	padding-top:5px;	padding-right:17px;	}
#header div.top p.top_info a 		{	color:#666;		}
#header div.top p.top_info a:hover 	{	color:#000000;	text-decoration: underline;		}
#header div.top p.top_info em 		{	color:#3876A4;	font-weight:bold;				}
#header div.top p.top_menu 			{	font-size:7pt;	text-align:right;	padding-top:20px;	padding-right:17px;	}
#header div.top p.top_menu a 		{	color:#666;		}
#header div.top p.top_menu a:hover 	{	color:#000000;	text-decoration: underline;		}
#header div.top p.top_menu em 		{	color:#3876A4;	font-weight:bold;				}

#header div.menu 					{	width:950px;	margin-top:2px;	background:url(/web/imgs/menu_bg.gif) left top repeat-x;	text-align:center;		height:38px;	}
#header div.menu ul 				{	line-height:35px;	}
#header div.menu li 				{	color:#FFF;	display: inline;	font-size: 10pt; 	font-weight:bold;	background:url(/web/imgs/menu_line.gif) right top no-repeat;	padding-right:60px;	padding-left:60px;	}
#header div.menu li.last 			{	background:none;	}
#header div.menu li a 				{	color:#fff;			}
#header div.menu li a:hover 		{	color:#000000;		text-decoration: underline;			}
